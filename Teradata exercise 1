DATABASE ua_dillards;
HELP TABLE DEPTINFO;
HELP COLUMN DEPTINFO.DEPT;
SHOW table DEPTINFO;

SELECT TOP 10 *
FROM strinfo;
 
SELECT TOP 10 *
FROM strinfo
ORDER BY city ASC;
 
SELECT TOP 10 *
FROM strinfo
ORDER BY city DESC;
 
SELECT *
FROM strinfo
SAMPLE 10;
 
SELECT *
FROM strinfo
SAMPLE .10;
 
SELECT *
FROM TRNSACT
WHERE SKU=3631365
ORDER BY ORGPRICE DESC;

SELECT COLOR
FROM SKUINFO
WHERE BRAND='LIZ CLAI'
ORDER BY SKU DESC;

SELECT DEPTDESC
FROM DEPTINFO
WHERE DEPTDESC LIKE 'E%';

SELECT TOP 100 DATE, ORGPRICE, SALEPRICE
FROM TRNSACT
WHERE SALEPRICE <> ORGPRICE
ORDER BY DATE;

SELECT TOP 10 *
FROM TRNSACT
WHERE SALEDATE > '2004-08-01' AND SALEDATE < '2004-08-10'
ORDER BY ORGPRICE DESC, SPRICE DESC;

SELECT DISTINCT BRAND
FROM SKUINFO
WHERE BRAND LIKE '%LIZ%';
